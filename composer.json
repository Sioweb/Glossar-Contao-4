{
    "name": "sioweb/glossar",
    "type": "contao-bundle",
    "description": "Glossar for Contao",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha Weidner",
            "email": "support@sioweb.de"
        }
    ],
    "require": {
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Sioweb\\GlossarBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Sioweb\\GlossarBundle\\ContaoManager\\Plugin"
    }
}
